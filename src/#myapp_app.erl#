-module(myapp_app).

-behaviour(application).

%% Application callbacks
-export([start/2, stop/1]).



%% ===================================================================
%% Application callbacks
%% ===================================================================

start(_StartType, _StartArgs) ->
    cynober_sup:start_link(),
    ok.

stop(_State) ->
    ok.


-ifdef(TEST).

application_start_test() ->
    ok = application:start(cynober).

-endif().
